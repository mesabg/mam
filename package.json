{
  "name": "mam",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "webpack-dev-server --port=4200 --config=webpack.dev.config.js",

    "dev-build": "webpack --optimize-max-chunks=true --optimize-min-chunk-size=true --optimize-minimize=true --config=webpack.dev.config.js",
    "prepredev": "webpack --optimize-max-chunks=true --optimize-min-chunk-size=true --optimize-minimize=true --config=webpack.dev.config.js",
    "predev": "ng build --app universal --env=dev --output-hashing=none",
    "dev": "node server.js",

    "prod-build": "webpack --optimize-max-chunks=true --optimize-min-chunk-size=true --optimize-minimize=true --config=webpack.prod.config.js",
    "prepreprod": "webpack --optimize-max-chunks=true --optimize-min-chunk-size=true --optimize-minimize=true --config=webpack.prod.config.js",
    "preprod": "ng build --prod --env=prod --app universal --output-hashing=none",
    "prod": "node server.js",

    "test": "karma start ./karma.conf.js",
    "lint": "ng lint",

    "prepree2e": "npm start",
    "pree2e": "webdriver-manager update --standalone false --gecko false --quiet",
    "e2e": "protractor ./protractor.conf.js",

    "dev-eject": "ng eject --base-href=/ --output-path=dist/ --env=dev --aot --force",
    "prod-eject": "ng eject --base-href=/ --output-path=dist/ --env=prod --aot --force",
    "server-eject": "ng eject --prod --app universal --output-hashing=none --force",

    "preseo": "ng build --prod && ngc",
    "seo": "ts-node src/server.ts"
  },
  "private": true,
  "engines": {
    "node": "8.5.0",
    "npm": "5.4.2"
  },
  "dependencies": {
    "@angular/animations": "4.2.3",
    "@angular/cli": "1.4.9",
    "@angular/common": "4.2.3",
    "@angular/compiler": "4.2.3",
    "@angular/compiler-cli": "4.2.3",
    "@angular/core": "4.2.3",
    "@angular/forms": "4.2.3",
    "@angular/http": "4.2.3",
    "@angular/platform-browser": "4.2.3",
    "@angular/platform-browser-dynamic": "4.2.3",
    "@angular/platform-server": "4.2.3",
    "@angular/router": "4.2.3",
    "@nguniversal/express-engine": "^1.0.0-beta.3",
    "@types/jasmine": "2.5.38",
    "@types/node": "~6.0.60",
    "autoprefixer": "6.5.3",
    "bootstrap": "4.0.0-alpha.6",
    "classlist.js": "1.1.20150312",
    "codelyzer": "~2.0.0",
    "compression-webpack-plugin": "1.0.1",
    "core-js": "2.4.1",
    "css-loader": "0.28.1",
    "cssnano": "3.10.0",
    "exports-loader": "0.6.3",
    "file-loader": "0.10.0",
    "font-awesome": "4.7.0",
    "http-serve": "^1.0.1",
    "intl": "1.2.5",
    "istanbul-instrumenter-loader": "2.0.0",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "~3.2.0",
    "jquery": "3.2.1",
    "json-loader": "0.5.4",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.0.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "0.2.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "0.2.2",
    "less-loader": "4.0.5",
    "postcss-loader": "1.3.3",
    "postcss-url": "5.1.2",
    "protractor": "~5.1.0",
    "raw-loader": "0.5.1",
    "rxjs": "5.5.0",
    "sass-loader": "6.0.6",
    "script-loader": "0.7.0",
    "slick-carousel": "1.8.1",
    "source-map-loader": "0.2.0",
    "style-loader": "0.13.1",
    "stylus-loader": "3.0.1",
    "ts-node": "2.0.0",
    "tslint": "4.5.0",
    "typescript": "^2.3.4",
    "url-loader": "0.5.7",
    "web-animations-js": "2.3.1",
    "webpack": "3.1.0",
    "webpack-dev-server": "2.9.3",
    "zone.js": "0.8.4"
  },
  "devDependencies": {}
}
