    "slick-carousel": "^1.8.1",
    "source-map-loader": "^0.2.0",
    "style-loader": "^0.13.1",
    "stylus-loader": "^3.0.1",
    "ts-node": "3.2.0",
    "tslint": "5.7.0",
    "typescript": "2.3.3",
    "url-loader": "^0.6.2",
    "web-animations-js": "^2.3.1",
    "webpack": "~3.7.1",
