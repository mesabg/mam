{
  "name": "mam",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "clean": "npm run clean:artifacts & rimraf node_modules/.cache coverage test-report.xml",
    "clean:artifacts": "rimraf .cache .server/server* .temp public",
    "build:universal-dev": "npm run clean:artifacts && gulp build:universal-dev --color",
    "build:universal-prod": "npm run clean:artifacts && gulp build:universal-prod --color",
    "serve": "nodemon ./.server/server.js",
    "serve:prod": "node ./.server/server.js",
    "ng": "ng",
    "start": "webpack-dev-server --port=4200",
    "build": "webpack",
    "test": "karma start ./karma.conf.js",
    "lint": "ng lint",
    "e2e": "protractor ./protractor.conf.js",
    "dev-build": "webpack --optimize-max-chunks=true --optimize-min-chunk-size=true --optimize-minimize=true --config=webpack.dev.config.js",
    "predev": "npm run dev-build && ng build --app universal --env=dev --output-hashing=none",
    "dev": "node server.js",
    "prod-build": "webpack --optimize-max-chunks=true --optimize-min-chunk-size=true --optimize-minimize=true --config=webpack.prod.config.js",
    "preprod": "npm run prod-build && ng build --prod --env=prod --app universal --output-hashing=none",
    "prod": "node server.js",
    "dev-eject": "ng eject --base-href=/ --output-path=dist/ --env=dev --aot --force",
    "prod-eject": "ng eject --base-href=/ --output-path=dist/ --env=prod --aot --force",
    "server-eject": "ng eject --prod --app universal --output-hashing=none --force",
    "pree2e": "webdriver-manager update --standalone false --gecko false --quiet",
    "preprod-bundle": "npm install --verbose",
    "prod-bundle": "npm run prod-build",
    "deploy": "http-serve ./dist -g true"
  },
  "private": true,
  "engines": {
    "node": "8.5.0",
    "npm": "5.4.2"
  },
