{
  "name": "mam",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "webpack-dev-server --port=4200",
    "build": "webpack",
    "test": "karma start ./karma.conf.js",
    "lint": "ng lint",
    "e2e": "protractor ./protractor.conf.js",
    "dev-build": "webpack --optimize-max-chunks=true --optimize-min-chunk-size=true --optimize-minimize=true --config=webpack.dev.config.js",
    "predev": "npm run dev-build && ng build --app universal --env=dev --output-hashing=none",
    "dev": "node server.js",
    "prod-build": "webpack --optimize-max-chunks=true --optimize-min-chunk-size=true --optimize-minimize=true --config=webpack.prod.config.js",
    "preprod": "npm run prod-build && ng build --prod --env=prod --app universal --output-hashing=none",
    "prod": "node server.js",
    "dev-eject": "ng eject --base-href=/ --output-path=dist/ --env=dev --aot --force",
    "prod-eject": "ng eject --base-href=/ --output-path=dist/ --env=prod --aot --force",
    "server-eject": "ng eject --prod --app universal --output-hashing=none --force",
    "pree2e": "webdriver-manager update --standalone false --gecko false --quiet",
    "deploy": "http-serve ./dist -g true -p 80"
  },
  "private": true,
  "engines": {
    "node": "8.5.0",
    "npm": "5.4.2"
  },
  "dependencies": {
    "@angular/animations": "4.2.4",
    "@angular/cli": "1.4.9",
    "@angular/common": "4.2.4",
    "@angular/compiler": "4.2.4",
    "@angular/compiler-cli": "4.2.4",
    "@angular/core": "4.2.4",
    "@angular/forms": "4.2.4",
    "@angular/http": "4.2.4",
    "@angular/language-service": "4.2.4",
    "@angular/platform-browser": "4.2.4",
    "@angular/platform-browser-dynamic": "4.2.4",
    "@angular/platform-server": "4.2.4",
    "@angular/router": "4.2.4",
    "@nguniversal/express-engine": "^1.0.0-beta.3",
    "@types/jasmine": "2.5.53",
    "@types/jasminewd2": "2.0.2",
    "@types/node": "6.0.60",
    "autoprefixer": "^6.5.3",
    "bootstrap": "^4.0.0-alpha.6",
    "circular-dependency-plugin": "^3.0.0",
    "classlist.js": "^1.1.20150312",
    "codelyzer": "3.2.0",
    "compression-webpack-plugin": "^1.0.1",
    "copy-webpack-plugin": "^4.1.1",
    "core-js": "2.4.1",
    "css-loader": "^0.28.1",
    "cssnano": "^3.10.0",
    "es6-shim": "^0.35.3",
    "exports-loader": "^0.6.3",
    "file-loader": "^1.1.5",
    "font-awesome": "^4.7.0",
    "html-webpack-plugin": "^2.29.0",
    "http-serve": "^1.0.1",
    "intl": "^1.2.5",
    "istanbul-instrumenter-loader": "^2.0.0",
    "jasmine-core": "2.6.2",
    "jasmine-spec-reporter": "4.1.0",
    "jquery": "^3.2.1",
    "jsdom": "^11.3.0",
    "karma": "1.7.0",
    "karma-chrome-launcher": "2.1.1",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "1.2.1",
    "karma-jasmine": "1.1.0",
    "karma-jasmine-html-reporter": "0.2.2",
    "less-loader": "^4.0.5",
    "mydatepicker": "^2.4.0",
    "postcss-loader": "^1.3.3",
    "postcss-url": "^5.1.2",
    "protractor": "5.1.2",
    "raw-loader": "^0.5.1",
    "rxjs": "5.4.2",
